{
  "directories": [
    "doc",
    "src"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        ".gitignore",
        ".md",
        "Tester",
        "Tests",
        "tests"
      ],
      "in": "vendor"
    }
  ],
  "compactors": [
    "Herrera\\Box\\Compactor\\Json",
    "Herrera\\Box\\Compactor\\Php"
  ],
  "compression": "GZ",
  "git-version": "package_version",
  "main": "bin/pimcore.php",
  "output": "build/pimcore.phar",
  "stub": true,
  "chmod": "0755"
}
